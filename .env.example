
# Configurações do Supabase
# Substitua pelos valores reais do seu projeto
SUPABASE_URL=https://your-project-ref.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here


# Access Token para CLI (não versionar em produção)
SUPABASE_ACCESS_TOKEN=

# Configurações do serviço de email (opcional)
# Para usar um serviço como SendGrid, Mailgun, etc.
MAIL_API_URL=your_mail_service_url_here
MAIL_API_KEY=your_mail_api_key_here

# Configuração da autenticação (para testes)
SUPABASE_ANON_KEY=your_anon_key_here
